{
  "name": "lgportfolio",
  "version": "1.0.0",
  "description": "An architect portfolio",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MadalenaGoncalves/lgportfolio.git"
  },
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "build-css": "node-sass --include-path public/sass/styles.scss build/css/styles.css",
    "sass": "node-sass -w scss/styles.scss -o build/css/",
    "uglify": "uglifyjs public/js/app.js public/js/controllers/* -m -c -o build/scripts.min.js",
    "build": "npm run build-css && npm run uglify"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-aria": "^1.6.4",
    "angular-material": "^1.1.4",
    "angular-messages": "^1.6.4",
    "async": "^1.5.2",
    "body-parser": "^1.17.1",
    "ejs": "^2.4.2",
    "ejs-locals": "^1.0.2",
    "express": "^4.14.1",
    "mongodb": "^2.1.21",
    "mongoose": "^4.5.10",
    "node-mailer": "^0.1.1",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "uglifyjs": "^2.4.11"
  },
